{
  "expo": {
    "name": "GrowUp Yourself",
    "slug": "growup-yourself",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#0A6CF0"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTabletMode": true,
      "bundleIdentifier": "com.growupyourself.app"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#0A6CF0"
      },
      "package": "com.growupyourself.app"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "color": "#0A6CF0"
        }
      ],
      "expo-router",
      "expo-sqlite"
    ],
    "scheme": "growup-yourself"
  }
}
