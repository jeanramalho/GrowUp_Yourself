{
  "expo": {
    "name": "GrowUp Yourself",
    "slug": "growup-yourself",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0A6CF0"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTabletMode": true,
      "bundleIdentifier": "com.growupyourself.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0A6CF0"
      },
      "package": "com.growupyourself.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#0A6CF0"
        }
      ],
      [
        "expo-contacts",
        {
          "contactsPermission": "Precisamos acessar seus contatos para sugerir pessoas especiais para vocÃª se conectar."
        }
      ],
      "expo-router",
      "expo-sqlite",
      "@react-native-community/datetimepicker"
    ],
    "scheme": "growup-yourself"
  }
}